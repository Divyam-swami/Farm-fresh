            />
          </div>
          <div class="d-flex align-items-center justify-content-between">
            <h3><%= frontproductdata[i].productname %></h3>
            <span class="fw-bold"><%= frontproductdata[i].price %>/kg</span>
          </div>
          <div class="product-detail">
            <a href="/details/<%= frontproductdata[i]._id %>" class="btn main-btn">View details</a>
          </div>
          
        </div>
          </div>
          <%} %>
